{
  "name": "@peter.naydenov/walk",
  "description": "Deep copy with fine control during the process. Can be used also as a deep forEach function",
  "version": "4.2.3",
  "license": "MIT",
  "author": "Peter Naydenov",
  "main": "./dist/walk.umd.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/walk.esm.mjs",
      "require": "./dist/walk.cjs",
      "default": "./dist/walk.umd.js"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*",
    "./src/*": "./src/*"
  },
  "scripts": {
    "test": "mocha test",
    "cover": "c8 mocha test",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PeterNaydenov/walk.git"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "c8": "^10.1.2",
    "chai": "5.1.2",
    "mocha": "10.8.2",
    "rollup": "^4.27.3"
  },
  "c8": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "node_modules",
      "test",
      "test-data"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "browserslist": "> 2%, not dead"
}
